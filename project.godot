; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="Minigame Mania"
config/description="Minigames that make you feel outta time!"
run/main_scene="res://Scenes/main.tscn"
boot_splash/image="res://Assets/textures/overlay_bg.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

GlobalVariables="*res://Scripts/GlobalVariables.gd"

[display]

window/size/width=576
window/size/height=1024
window/size/resizable=false
window/size/test_width=288
window/size/test_height=512
window/handheld/orientation="portrait"
window/stretch/mode="viewport"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

2d/snapping/use_gpu_pixel_snap=true
environment/default_clear_color=Color( 0.878431, 0.878431, 0.878431, 1 )
environment/default_environment="res://default_env.tres"
quality/2d/use_pixel_snap=true
